{
  "name": "mio",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "email": "ferdiardiansa@gmail.com",
    "name": "ferdiardiansa",
    "url": "https://github.com/itsferdiardiansa"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "install:all": "pnpm install --recursive",
    "lint": "eslint --config eslint.config.js --no-cache",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write 'apps/**/*.{js,ts}' 'packages/**/*.{js,ts}'",
    "format:check": "prettier --check 'apps/**/*.{js,ts}' 'packages/**/*.{js,ts}'",
    "test": "jest --passWithNoTests",
    "test:watch": "pnpm run test --watch",
    "test:apps": "pnpm run test --selectProjects apps",
    "test:packages": "pnpm run test --selectProjects packages",
    "test:coverage": "bash ./scripts/ci/test-coverage.sh",
    "type-check": "tsc --noEmit",
    "clean": "bash ./scripts/cleanup/clean.sh",
    "validate": "pnpm lint && pnpm type-check && pnpm format:check && pnpm test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@nx/js": "20.3.3",
    "@swc-node/register": "~1.10.0",
    "@swc/core": "~1.10.0",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nx": "20.3.3",
    "nyc": "^17.1.0",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.7.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
