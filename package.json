  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "install:all": "pnpm install --recursive",
    "lint": "eslint --config eslint.config.js --no-cache",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write 'apps/**/*.{js,ts}' 'packages/**/*.{js,ts}'",
    "format:check": "prettier --check 'apps/**/*.{js,ts}' 'packages/**/*.{js,ts}'",
    "test": "jest --passWithNoTests",
    "test:watch": "pnpm run test --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf apps/**/{dist,build} packages/**/{dist,build}",
    "validate": "pnpm lint && pnpm type-check && pnpm format:check && pnpm test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.13.0",
    "@nx/js": "20.0.6",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lerna": "^4.0.0",
    "lint-staged": "^15.2.10",
    "nx": "20.0.6",
    "nyc": "^17.1.0",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.5.2"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
